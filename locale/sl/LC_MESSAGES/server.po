#: lib/senders/templates/cadReminderFirst.txt:1 lib/senders/email.js:3023
msgid "Here's your reminder to sync devices."
msgstr "Pošiljamo vam opomnik za sinhronizacijo naprav."

#: lib/senders/templates/cadReminderFirst.txt:2 lib/senders/email.js:3027
msgid "It takes two to sync. Syncing another device with Firefox privately keeps your bookmarks, passwords and other Firefox data the same everywhere you use Firefox."
msgstr "Za sinhronizacijo sta potrebna dva. Sinhronizacija druge naprave s Firefoxom vam omogoča zasebno usklajevanje zaznamkov, gesel in drugih podatkov, kjerkoli uporabljate Firefox."

#: lib/senders/templates/cadReminderSecond.txt:1 lib/senders/email.js:3024
msgid "Last reminder to sync devices!"
msgstr "Zadnji opomnik za sinhronizacijo naprav!"

#: lib/senders/templates/cadReminderSecond.txt:2 lib/senders/email.js:3030
msgid "Syncing another device with Firefox privately keeps your bookmarks, passwords and other Firefox data the same everywhere you use Firefox."
msgstr "Sinhronizacija druge naprave s Firefoxom vam omogoča zasebno usklajevanje zaznamkov, gesel in drugih podatkov, kjerkoli uporabljate Firefox."

#: lib/senders/templates/downloadSubscription.html:1 lib/senders/templates/downloadSubscription.txt:1

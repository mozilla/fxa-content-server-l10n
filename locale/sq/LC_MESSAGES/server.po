#: templates/verification_reminder.html:9 templates/verify.html:9
#: templates/verify_reminder_first.html:9
#: templates/verify_reminder_second.html:9
msgid "Verify your email to finish your Firefox Account registration"
msgstr ""
"Verifikoni email-in tuaj që të përfundoni regjistrimin e Llogarisë suaj "
"Firefox"

#: templates/verification_reminder.txt:3
msgid "Activate Now:"
msgstr "Aktivizojeni Tani:"


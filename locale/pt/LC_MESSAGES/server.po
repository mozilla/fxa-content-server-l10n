#: templates/password_changed.html:2 templates/password_changed.txt:1
msgid "Password changed successfully"
msgstr "Palavra-passe alterada com sucesso"

#: templates/password_changed.html:3 templates/password_changed.txt:2
msgid "Your Firefox Account password has been successfully changed."
msgstr "A palavra-passe da sua Conta Firefox foi alterada com sucesso."

#: templates/password_changed.txt:3
msgid ""
"This is an automated email; if you didn’t change the password to your "
"Firefox Account, you should reset it immediately at %(resetLink)s."
msgstr ""
"Este é um email automático; se não alterou a palavra-passe da sua Conta "
"Firefox, deve repor a mesma imediatamente através de %(resetLink)s."

#: templates/password_reset.html:2 templates/password_reset.txt:1
msgid "Password reset successfully"
msgstr "Palavra-passe reposta com sucesso"

#: templates/password_reset.html:3 templates/password_reset.txt:2
msgid "Your Firefox Account password has been successfully reset."
msgstr "A palavra-passe da sua Conta Firefox foi reposta com sucesso."

#: templates/password_reset.txt:3
msgid ""
"This is an automated email; if you didn’t reset the password to your Firefox "
"Account, you should reset it immediately at %(resetLink)s."
msgstr ""
"Este é um email automático; se não repôs a palavra-passe da sua Conta "
"Firefox, deve repor a mesma imediatamente através de %(resetLink)s."

#: templates/post_verify.html:2 templates/post_verify.txt:1
msgid "Account verified!"
msgstr "Conta confirmada!"


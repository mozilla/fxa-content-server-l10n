msgid "Manage account"
msgstr "Konto rjadować"

#: lib/senders/templates/post_add_account_recovery.txt:2
msgid "You have successfully generated an account recovery key for your Firefox Account from the following device:"
msgstr "Sće wuspěšnje nowy wobnowjenski kluč za swoje konto Firefox ze slědowaceho grata wutworił:"

#: lib/senders/templates/post_add_account_recovery.txt:4
msgid "If this was not you, revoke key."
msgstr "Jeli wy to njejsće był, wotwołajće kluč."

#: lib/senders/templates/post_add_account_recovery.txt:5 lib/senders/templates/post_add_two_step_authentication.txt:4 lib/senders/templates/post_change_primary.txt:3
#: lib/senders/templates/post_consume_recovery_code.txt:4 lib/senders/templates/post_new_recovery_codes.txt:4 lib/senders/templates/post_remove_account_recovery.txt:5
#: lib/senders/templates/post_remove_secondary.txt:3 lib/senders/templates/post_remove_two_step_authentication.txt:4 lib/senders/templates/post_verify_secondary.txt:3
